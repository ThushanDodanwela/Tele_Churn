					},
					{
						"name": "Online_Backup",
						"type": "bit"
					},
					{
						"name": "Monthly_Charge",
						"type": "decimal",
						"precision": 10,
						"scale": 2
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "DIM_Broadband"
				}
			},
			"dependsOn": [

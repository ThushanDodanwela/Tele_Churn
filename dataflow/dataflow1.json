{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "broadbandDF",
						"type": "DatasetReference"
					},
					"name": "broadband"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> broadband",
				"broadband sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Broadband_SK as integer,",
				"          Broadband_Plan_Name as string,",
				"          Data_Allowance as integer,",
				"          Online_Security as boolean,",
				"          Online_Backup as boolean,",
				"          Monthly_Charge as decimal(10,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Broadband_SK = Broadband_Plan_ID,",
				"          Broadband_Plan_Name,",
				"          Data_Allowance,",
				"          Online_Security = {Online Security},",
				"          Online_Backup = {Online Backup},",
				"          Monthly_Charge",
				"     )) ~> sink1"
			]
		}
	}
}
{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "broadbandDF",
						"type": "DatasetReference"
					},
					"name": "broadband"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Broadband_Plan_ID as string,",
				"          Broadband_Plan_Name as string,",
				"          Data_Allowance as string,",
				"          {Online Security} as string,",
				"          {Online Backup} as string,",
				"          Monthly_Charge as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> broadband",
				"broadband sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Broadband_SK as integer,",
				"          Broadband_Plan_Name as string,",
				"          Data_Allowance as integer,",
				"          Online_Security as boolean,",
				"          Online_Backup as boolean,",
				"          Monthly_Charge as decimal(10,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}